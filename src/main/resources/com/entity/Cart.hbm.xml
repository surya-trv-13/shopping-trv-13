<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping package="com.entity">
    	<class name="Cart" table="cart">
    		<id name="productId" column="product_id">
    			<generator class="native"></generator>
    		</id>
    		<property name="productName" column="p_name" type="java.lang.String"/>
    		<property name="price" column="p_price" type="java.lang.Double"/>
    		<property name="quantity" column="quantity" type="java.lang.Integer"/>
    	</class>
    </hibernate-mapping>